<div id="signinContainer" class="container hide-item">
	<div class="acct-container-inner">
		<div class="form-group">
			<label for="username">Username</label>
			<input type="text" id="username" class="form-control" placeholder="username">
		</div>
		<div class="form-group">
			<label for="password">Password</label>
			<input type="password" id="password" class="form-control" placeholder="password">
		</div>
		<button id="signin-submit" class="btn btn-primary">Sign In</button>
	</div>
</div>
<div id="createContainer" class="container hide-item">
	<div class="acct-container-inner">
		<div class="form-group">
			<label for="create-username">Username</label>
			<input type="text" id="create-username" class="form-control" placeholder="username"/>
		</div>	
		<div class="form-group">
			<label for="create-password">Password</label>
			<input type="password" id="create-password" class="form-control" placeholder="password"/>
		</div>	
		<div class="form-group">
			<label for="create-first">First Name</label>
			<input type="text" id="create-first" class="form-control" placeholder="first"/>
		</div>	
		<div class="form-group">
			<label for="create-last">Last Name</label>
			<input type="text" id="create-last" class="form-control" placeholder="last"/>
		</div>	
		<div class="form-group">
			<label for="create-email">Email</label>
			<input type="email" id="create-email" class="form-control" placeholder="email@example.com"/>
		</div>	
		<button id="create-submit" class="btn btn-primary">Create Account</button>
	</div>
</div>